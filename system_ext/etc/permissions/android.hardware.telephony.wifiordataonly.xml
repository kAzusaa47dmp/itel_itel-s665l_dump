<?xml version="1.0" encoding="utf-8"?>

<permissions>
    <customized-restricted-app config="ro.radio.noril"  value="1">
        <path name="Dialer"/>
        <path name="SprdDialer"/>
        <path name="SprdDialerGo"/>
        <path name="GoogleDialer"/>
        <path name="GoogleDialerGo"/>
        <path name="Stk"/>
