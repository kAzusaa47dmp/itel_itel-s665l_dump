            <Limit name="block-size" value="16x16" />
            <Limit name="block-count" range="1-32640" />
            <Limit name="blocks-per-second" range="1-1728000" />
            <Limit name="bitrate" range="1-50000000" />
            <Limit name="concurrent-instances" max="20" />
