<?xml version="1.0"?>
<!-- Subsystem log settings -->
<subsys_settings>

  <cellular_modem>
    <log_mode>none</log_mode>
    <dest>local</dest>
    <compression>none</compression>
    <ps>off</ps>
    <ps_level>0</ps_level>
    <ps_pcm>off</ps_pcm>
    <ip>off</ip>
    <ip_len>10000</ip_len>
    <dump>on</dump>
    <etb>off</etb>
    <phy_dest>ps</phy_dest>
    <phy>off</phy>
    <phy_level>1</phy_level>
    <phy_pcm>off</phy_pcm>
    <iq>off</iq>
    <iq_item>0</iq_item>
    <iq_pos>0</iq_pos>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>1024</int_cap>
      <file>256</file>
      <recycle>on</recycle>
    </storage>
  </cellular_modem>

  <connectivity>
    <dest>local</dest>
    <compression>none</compression>
    <wifi_bt>off</wifi_bt>
    <wifi_bt_level>0</wifi_bt_level>
    <gnss>off</gnss>
    <wcn_reset_state>reset</wcn_reset_state>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>512</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </connectivity>

  <audio_dsp>
    <log>off</log>
    <pcm>off</pcm>
    <dest>local</dest>
    <agdspdest>ap</agdspdest>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>256</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </audio_dsp>

  <vdsp>
    <log>off</log>
    <dest>local</dest>
    <compression>none</compression>
    <level>debug</level>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>256</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </vdsp>

  <pmsh>
    <log>off</log>
    <compression>none</compression>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>256</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </pmsh>

  <sensor_hub>
    <log>off</log>
    <compression>none</compression>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>256</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </sensor_hub>

</subsys_settings>
